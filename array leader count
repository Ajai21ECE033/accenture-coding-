// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    int arr[]={7,9,5,2,8,7};
    int sup=0;
    int count=0;
    int n=6;
    
    for(int i=n-1;i>=0;i--){
        if(arr[i]>sup){
            count++;
            sup=arr[i];
        }
    }
    cout<<count;
   
    return 0;
}
